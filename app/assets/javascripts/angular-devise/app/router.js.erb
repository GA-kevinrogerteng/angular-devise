App.config(['$routeProvider', function($routeProvider) {
  $routeProvider.when('/home', {templateUrl: '<%= asset_path "home.html" %>'});
  $routeProvider.when('/register', {templateUrl: '<%= asset_path "register.html" %>', controller: RegistrationController});
  $routeProvider.when('/login', {templateUrl: '<%= asset_path "login.html" %>', controller: SessionController});
  $routeProvider.otherwise({redirectTo: '/home'});
}]);